<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zhxh.xtouchdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TouchDelegateActivity" />
        <activity android:name=".MoveLoggerActivity" />
        <activity android:name=".MultitouchActivity" />
        <activity android:name=".TouchForwardActivity" />
        <activity android:name=".TouchInterceptActivity" />
        <activity android:name=".TouchListenerActivity" />
        <activity android:name=".TwoDimensionGestureScrollActivity" />
        <activity android:name=".TwoDimensionScrollActivity" />
    </application>

</manifest>